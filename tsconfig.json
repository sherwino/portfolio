{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "lib": ["ESNext", "dom"],
    "sourceMap": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./src/",
    "rootDir": "./src"
  },
  "exclude": ["node_modules", "lib", "**/__tests__", "Resources", "public"]
}
