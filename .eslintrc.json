{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "double", { "avoidEscape": true }],
    "semi": [2, "always"],
    "comma-dangle": [2, "always-multiline"]
  }
}
